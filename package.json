{
  "name": "somnia-ai-agent-framework",
  "version": "1.0.0",
  "description": "A comprehensive framework, SDK, and monitoring tools for designing, deploying, and scaling AI agents on Somnia Network",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "dev": "ts-node src/index.ts",
    "test": "jest",
    "deploy:contracts": "hardhat run scripts/deploy.ts --network somnia",
    "verify": "hardhat verify --network somnia",
    "start:monitor": "ts-node src/monitoring/server.ts",
    "start:dashboard": "cd dashboard && npm run dev",
    "lint": "eslint src --ext .ts",
    "format": "prettier --write \"src/**/*.ts\""
  },
  "keywords": [
    "somnia",
    "ai",
    "agents",
    "blockchain",
    "web3",
    "framework",
    "sdk",
    "monitoring"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "ethers": "^6.9.0",
    "viem": "^2.7.0",
    "express": "^4.18.2",
    "ws": "^8.14.2",
    "dotenv": "^16.3.1",
    "axios": "^1.6.2",
    "winston": "^3.11.0",
    "zod": "^3.22.4",
    "openai": "^4.20.1",
    "cors": "^2.8.5",
    "ipfs-http-client": "^60.0.1"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@types/express": "^4.17.21",
    "@types/ws": "^8.5.9",
    "@types/cors": "^2.8.17",
    "@nomicfoundation/hardhat-toolbox": "^4.0.0",
    "@nomicfoundation/hardhat-verify": "^2.0.0",
    "hardhat": "^2.19.2",
    "typescript": "^5.3.2",
    "ts-node": "^10.9.1",
    "@typescript-eslint/eslint-plugin": "^6.13.0",
    "@typescript-eslint/parser": "^6.13.0",
    "eslint": "^8.54.0",
    "prettier": "^3.1.0",
    "jest": "^29.7.0",
    "@types/jest": "^29.5.8",
    "ts-jest": "^29.1.1"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}

