{
  "name": "somnia-agent-kit",
  "version": "3.0.7",
  "description": "A comprehensive SDK and toolkit for building, deploying, and managing AI agents on Somnia Network",
  "private": true,
  "packageManager": "pnpm@8.15.0",
  "scripts": {
    "build": "pnpm -r --filter='@somnia/*' run build",
    "build:kit": "pnpm --filter @somnia/agent-kit run build",
    "dev": "pnpm --filter @somnia/agent-kit run dev",
    "test": "pnpm -r run test",
    "test:kit": "pnpm --filter @somnia/agent-kit run test",
    "lint": "eslint packages/*/src --ext .ts",
    "lint:fix": "eslint packages/*/src --ext .ts --fix",
    "format": "prettier --write \"packages/**/src/**/*.ts\" \"contracts/contracts/**/*.sol\"",
    "typecheck": "pnpm -r run typecheck",
    "clean": "pnpm -r run clean && rm -rf node_modules .turbo",
    "deploy:contracts": "pnpm --filter contracts run deploy"
  },
  "keywords": [
    "somnia",
    "ai",
    "agents",
    "agent-kit",
    "blockchain",
    "web3",
    "sdk",
    "toolkit",
    "llm"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.0.0",
    "eslint": "^8.57.0",
    "prettier": "^3.1.0",
    "typescript": "^5.3.2"
  },
  "engines": {
    "node": ">=18.0.0",
    "pnpm": ">=8.0.0"
  }
}
