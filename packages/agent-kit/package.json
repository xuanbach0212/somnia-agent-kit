{
  "name": "somnia-agent-kit",
  "version": "2.1.0",
  "description": "Production-ready SDK for building, deploying, and managing AI agents on Somnia blockchain",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "bin": {
    "somnia-agent": "./dist/cli/bin.js",
    "sak": "./dist/cli/bin.js"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist .turbo",
    "prepublishOnly": "pnpm run build"
  },
  "keywords": [
    "somnia",
    "ai-agents",
    "blockchain",
    "sdk",
    "web3",
    "llm",
    "ai",
    "agents",
    "smart-contracts",
    "ethereum",
    "typescript"
  ],
  "author": "Somnia Network",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/xuanbach0212/somnia-agent-kit.git",
    "directory": "packages/agent-kit"
  },
  "homepage": "https://github.com/xuanbach0212/somnia-agent-kit#readme",
  "bugs": {
    "url": "https://github.com/xuanbach0212/somnia-agent-kit/issues"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "deepmerge": "^4.3.1",
    "dotenv": "^17.2.3",
    "ethers": "^6.9.0",
    "express": "^4.18.2",
    "pino": "^8.17.0",
    "pino-pretty": "^10.3.0",
    "winston": "^3.11.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/node": "^20.10.0",
    "tsup": "^8.0.1",
    "typescript": "^5.3.2",
    "vitest": "^1.2.0"
  },
  "peerDependencies": {
    "ethers": "^6.0.0"
  }
}
